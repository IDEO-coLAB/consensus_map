{
  "presets": [
    ["env", {
      "modules": false,
      "useBuiltIns": "usage",
      "targets": {
        "ie": 9
      },
      "shippedProposals": true
    }],
    ["stage-2"]
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": true,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}

{
  "name": "vue-vuex-starter-kit",
  "version": "0.0.1",
  "description": "Get started with Vue, Vuex, and Vue-router. Build app with webpack.",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "clean": "rimraf dist",
    "clean:dll": "rimraf dll",
    "build": "rimraf dist && webpack -p --config ./config/webpack.config.prod.js --progress"
  },
  "betterScripts": {
    "start": {
      "command": "node ./server/bin/www",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
